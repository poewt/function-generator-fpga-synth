#! /opt/homebrew/Cellar/icarus-verilog/12.0/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/system.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_sys.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_textio.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/v2005_math.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x140f0b520 .scope module, "square_wave_tb" "square_wave_tb" 2 3;
 .timescale -9 -12;
v0x140f1b2e0_0 .var "clk", 0 0;
v0x140f1b390_0 .net "square_out", 7 0, v0x140f1b250_0;  1 drivers
S_0x140f07370 .scope module, "uut" "square_wave" 2 11, 3 1 0, S_0x140f0b520;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /OUTPUT 8 "square_out";
P_0x140f0b690 .param/l "HEIGHT" 0 3 8, +C4<00000001>;
P_0x140f0b6d0 .param/l "INCREMENT" 0 3 6, +C4<00000000000000000000000000000001>;
P_0x140f0b710 .param/l "WIDTH" 0 3 7, +C4<00000000000000000000000000001010>;
v0x140f074e0_0 .net "clk", 0 0, v0x140f1b2e0_0;  1 drivers
v0x140f1b120_0 .var "counter", 7 0;
v0x140f1b1c0_0 .var "flip", 0 0;
v0x140f1b250_0 .var/s "square_out", 7 0;
E_0x140f058b0 .event posedge, v0x140f074e0_0;
    .scope S_0x140f07370;
T_0 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x140f1b120_0, 0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x140f1b1c0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x140f07370;
T_1 ;
    %wait E_0x140f058b0;
    %load/vec4 v0x140f1b120_0;
    %pad/u 32;
    %cmpi/u 10, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_1.0, 5;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x140f1b120_0, 0;
    %load/vec4 v0x140f1b1c0_0;
    %inv;
    %store/vec4 v0x140f1b1c0_0, 0, 1;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x140f1b120_0;
    %pad/u 32;
    %addi 1, 0, 32;
    %pad/u 8;
    %assign/vec4 v0x140f1b120_0, 0;
T_1.1 ;
    %load/vec4 v0x140f1b1c0_0;
    %flag_set/vec4 8;
    %jmp/0 T_1.2, 8;
    %pushi/vec4 255, 0, 8;
    %jmp/1 T_1.3, 8;
T_1.2 ; End of true expr.
    %pushi/vec4 1, 0, 8;
    %jmp/0 T_1.3, 8;
 ; End of false expr.
    %blend;
T_1.3;
    %assign/vec4 v0x140f1b250_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x140f0b520;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x140f1b2e0_0;
    %inv;
    %store/vec4 v0x140f1b2e0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x140f0b520;
T_3 ;
    %vpi_call 2 19 "$dumpfile", "square_wave_tb.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x140f0b520 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x140f1b2e0_0, 0, 1;
    %delay 10000, 0;
    %delay 500000000, 0;
    %vpi_call 2 27 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
